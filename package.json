{
  "name": "reatom",
  "private": true,
  "description": "State manager with a focus of all needs",
  "scripts": {
    "postinstall": "npm run bootstrap",
    "bootstrap": "lerna bootstrap && lerna run rebuild",
    "build": "lerna run build",
    "test": "lerna run test",
    "size": "lerna run size",
    "prettify": "prettier --write **/*.{j,t}s !**/babel-plugin/test/**",
    "prettify:watch": "onchange **/*.{j,t}s !**/babel-plugin/test/** -- prettier --write {{changed}}",
    "publish": "lerna publish",
    "publish:next": "lerna publish --canary --preid next --dist-tag next --no-git-tag-version"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@commitlint/config-lerna-scopes": "^8.1.0",
    "@types/jest": "^24.0.18",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.1",
    "docsify-cli": "^4.3.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lerna": "^3.16.4",
    "microbundle": "^0.11.0",
    "onchange": "^6.0.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "author": "artalar",
  "license": "MIT",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/artalar/reatom.git"
  },
  "bugs": {
    "url": "https://github.com/artalar/reatom/issues"
  },
  "homepage": "https://artalar.github.io/reatom",
  "keywords": [
    "reatom",
    "@reatom",
    "state",
    "store",
    "flux",
    "atom",
    "reactive",
    "reactivity",
    "redux",
    "effector",
    "mobx",
    "state-manager",
    "state manager"
  ]
}
